{
    "services": [
        {
            "name": "Node",
            "start": "node node.js",
            "endpoint": "/hello",
            "port": 3301,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Spring-Boot-Jetty",
            "start": "java -jar spring-boot-jetty/target/gs-spring-boot-0.1.0.jar --server.port=3302",
            "endpoint": "/hello",
            "port": 3302,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Spring-Boot-Tomcat",
            "start": "java -jar spring-boot-tomcat/target/gs-spring-boot-0.1.0.jar --server.port=3303",
            "endpoint": "/hello",
            "port": 3303,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Green-Lightning-Telemetry",
            "start": "java -jar green-lightning/target/gl-hello.jar true 3304",
            "endpoint": "/hello",
            "port": 3304,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Green-Lightning-No-Telemetry",
            "start": "java -jar green-lightning/target/gl-hello.jar false 3305",
            "endpoint": "/hello",
            "port": 3305,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Micronaut",
            "start": "java -Dmicronaut.server.host=127.0.0.1 -Dmicronaut.server.port=3306 -jar micronaut-app/build/libs/micronaut-all.jar",
            "endpoint": "/hello",
            "port": 3306,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        },
        {
            "name": "Squall",
            "start": "java -jar squall/target/squall-hello.jar",
            "endpoint": "/hello",
            "port": 3307,
            "payload": {"name": "Rick", "happy": true, "age": 13}
        }
    ]
}
